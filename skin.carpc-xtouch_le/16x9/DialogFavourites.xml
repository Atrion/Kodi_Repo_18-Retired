<window type="dialog" id="134">
	<defaultcontrol always="true">450</defaultcontrol>
<!--<include>16x9_xPos_Relocation</include>-->
	<include>Window_OpenClose_Animation_Zoom</include>
	<coordinates>
		<system>1</system>
		<posx>180</posx>
		<posy>5</posy>
	</coordinates>
	<controls>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>920</width>
			<height>60</height>
			<texture border="4">DialogHeader.png</texture>
		</control>
		<control type="image">
			<posx>0</posx>
			<posy>60</posy>
			<width>920</width>
			<height>655</height>
			<texture border="4">dialog_back_black2.png</texture>
		</control>
		<control type="label">
			<description>header label</description>
			<posx>0</posx>
			<posy>0</posy>
			<include>WindowTitleCommons</include>
			<width>920</width>
			<height>60</height>
			<label>$LOCALIZE[1036]</label>
		</control>
		<control type="group">
			<posx>865</posx>
			<posy>0</posy>
			<include>DialogCloseButtonCommons</include>
		</control>
		<control type="list" id="450">
			<animation effect="slide" start="0,0" end="10,0" time="0" condition="!Control.IsVisible(60)">Conditional</animation>
			<posx>20</posx>
			<posy>65</posy>
			<width>870</width>
			<height>600</height>
			<onup>450</onup>
			<onleft>450</onleft>
			<onright>450</onright>
			<ondown>450</ondown>
			<pagecontrol>60</pagecontrol>
			<scrolltime>200</scrolltime>
			<itemlayout width="950" height="75">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>840</width>
					<height>75</height>
					<texture>MenuItemNF.png</texture>
				</control>
				<control type="label">
					<posx>90</posx>
					<posy>0</posy>
					<width>750</width>
					<height>75</height>
					<font>font30_title</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>white</textcolor>
					<selectedcolor>selected</selectedcolor>
					<label>$INFO[ListItem.Label]</label>
				</control>
				<control type="image">
					<posx>10</posx>
					<posy>5</posy>
					<width>65</width>
					<height>65</height>
					<aspectratio align="right">keep</aspectratio>
					<fadetime>IconCrossfadeTime</fadetime>
					<texture>$INFO[ListItem.Icon]</texture>
					<bordertexture border="4">ThumbBorderGrey.png</bordertexture>
					<bordersize>3</bordersize>
				</control>
			</itemlayout>
			<focusedlayout width="950" height="75">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>840</width>
					<height>75</height>
					<texture colordiffuse="$VAR[CustomColorFocus]">list_bg_fo.png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>840</width>
					<height>75</height>
					<texture>MenuItemNF.png</texture>
				</control>
				<control type="label">
					<posx>90</posx>
					<posy>0</posy>
					<width>750</width>
					<height>75</height>
					<font>font30_title</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>white</textcolor>
					<selectedcolor>selected</selectedcolor>
					<label>$INFO[ListItem.Label]</label>
				</control>
				<control type="image">
					<posx>10</posx>
					<posy>5</posy>
					<width>65</width>
					<height>65</height>
					<aspectratio align="right">keep</aspectratio>
					<fadetime>IconCrossfadeTime</fadetime>
					<texture>$INFO[ListItem.Icon]</texture>
					<bordertexture border="4">ThumbBorderGrey.png</bordertexture>
					<bordersize>3</bordersize>
				</control>
			</focusedlayout>
		</control>
		
<!-- Scrollbar with buttons -->
		<control type="group">
			<posx>870</posx>
			<posy>75</posy>
			<width>70</width>
			<height>630</height>
            <control type="button" id="6003">
				<visible>Control.IsVisible(60)</visible>
                <description>Button PageUp</description>
                <posx>0</posx>
                <posy>0</posy>
                <width>40</width>
                <height>40</height>
				<texturefocus colordiffuse="$VAR[CustomColorFocus]">arrow-up.png</texturefocus>
				<texturenofocus>arrow-up.png</texturenofocus>
                <colordiffuse>99FFFFFF</colordiffuse>
                <onclick>pageup(60)</onclick>
				<onup></onup>
				<onleft></onleft>
				<onright></onright>
				<ondown></ondown>
            </control>
			<control type="scrollbar" id="60">
